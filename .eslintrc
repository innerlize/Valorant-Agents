{
	"env": {
		"browser": true,
		"es2021": true,
		"node": true
	},
	"extends": ["eslint:recommended", "plugin:react/recommended"],
	"overrides": [
		{
			"files": ["**/*.test.js", "**/*.test.jsx"],
			"env": {
				"jest": true
			}
		}
	],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["react"],
	"settings": {
		"react": {
			"version": "detect"
		}
	},
	"rules": {}
}
